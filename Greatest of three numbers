// User function Template for C++

class Solution {
  public:
    int greatestOfThree(int A, int B, int C) {
        // code here
        //int greatest;
        if(A>B && A>C)
        {
           return A;
        }
        else if(B>A && B>C)
        {
            return B;
        }
        else if(C>A && C>B)
        {
            return C;
        }
    } 
};
